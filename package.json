{
  "name": "sf-311-whatsapp-bot",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev:frontend": "cd frontend && npm run dev",
    "dev:server": "python -m uvicorn server.main:app --reload --port 3001",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:server\"",
    "build": "cd frontend && npm run build",
    "install:frontend": "cd frontend && npm install",
    "install:server": "pip install -r server/requirements.txt",
    "install:all": "npm run install:frontend && npm run install:server"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
